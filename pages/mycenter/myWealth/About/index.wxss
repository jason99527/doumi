/* pages/childStore/index/index.wxss */
page{
  width: 100%;
  height:100%;
}
.box{
  border-radius:12rpx;
  box-shadow: 0 0 30rpx rgba(153, 153, 153, 0.2);
  width: calc(100% - 60rpx);
  min-height:calc(100% - 60rpx);
  margin: 30rpx;
  padding:1px
}
.Trophy-icon{
  width: 104rpx;
  height:126rpx;
  margin: 150rpx auto;
  display:block;
  margin-bottom: 10rpx
}
.text{
  font-size:32rpx;
  color:#333333;
  font-weight: bold;
  text-align: center;
  display: block;
}
.content{
  font-size:28rpx;
  margin-top: 50rpx;
  color:#666666;
  padding: 0 60rpx
}
.content-title{
  color:#666666;
  font-size:28rpx;
  font-weight: bold;
}
.ct{
  text-indent: 2em;
  text-align: left;
}